{
  "name": "svelte-bi",
  "description": "Bootstrap Icons for Svelte 5",
  "version": "0.0.6",
  "license": "MIT",
  "keywords": [
    "svelte",
    "icons",
    "bootstrap",
    "svg"
  ],
  "author": "Osman Nuri Okumu≈ü <onokumus@gmail.com> (https://github.com/onokumus)",
  "repository": "onokumus/svelte-bi",
  "bugs": {
    "url": "https://github.com/onokumus/svelte-bi/issues"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./Bi.svelte": {
      "types": "./dist/Bi.svelte.d.ts",
      "import": "./dist/Bi.svelte"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.8.4",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.2",
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.0",
    "bootstrap-icons": "^1.11.3",
    "publint": "^0.2.12",
    "svelte": "^5.2.8",
    "svelte-check": "^4.1.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  }
}